{
  "name": "server",
  "version": "2.0.0",
  "description": "server built with mongodb nodejs and changestream",
  "main": "index.ts",
  "author": "William Q",
  "license": "MIT",
  "scripts": {
    "server:dev": "set NODE_ENV=development&& nodemon",
    "server:prod": "NODE_ENV=production node ./dist/index.js",
    "client": "cd ../mern-project && set NODE_ENV=development&& npm start",
    "dev": "concurrently \"npm run server:dev\" \"npm run client\"",
    "build": "tsc && tsc-alias",
    "build:watch": "tsc && (concurrently \"tsc -w\" \"tsc-alias -w\")",
    "clean": "rimraf node_modules package-lock.json",
    "clean-client": "cd ../mern-project && npm clean",
    "rmPdf": "cd ./bin/pdfGenerator/pdf && rm ./*"
  },
  "imports": {
    "#root/*": "./*",
    "#query/*": "./lib/query/*",
    "#models/*": "./lib/models/*",
    "#routes/*": "./lib/routes/*",
    "#middleware/*":"./lib/middleware/*"
  },
  "dependencies": {
    "amazon-sp-api": "^0.8.0",
    "aws-sdk": "^2.1353.0",
    "axios": "^0.26.1",
    "bcryptjs": "^2.4.3",
    "config": "^3.3.9",
    "cookie-session": "^2.0.0",
    "cors": "^2.8.5",
    "dotenv": "^16.0.0",
    "exceljs": "^4.3.0",
    "express": "^4.17.1",
    "express-session": "^1.17.3",
    "fs-extra": "^10.1.0",
    "googleapis": "^95.0.0",
    "handlebars": "^4.7.7",
    "JSONStream": "^1.3.5",
    "jsonwebtoken": "^9.0.0",
    "moment": "^2.29.4",
    "mongodb": "^5.2.0",
    "mongoose": "^6.9.0",
    "multer": "^1.4.5-lts.1",
    "node-cron": "^3.0.0",
    "node-fetch": "^3.2.10",
    "node-rsa": "^1.1.1",
    "nodemailer": "^6.7.8",
    "passport": "^0.6.0",
    "passport-google-oauth20": "^2.0.0",
    "puppeteer": "^13.1.3",
    "socket.io": "^4.5.1",
    "tunnel-ssh": "^4.1.6"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.2",
    "@types/config": "^3.3.0",
    "@types/express": "^4.17.14",
    "@types/express-session": "^1.17.7",
    "@types/fs-extra": "^9.0.13",
    "@types/jsonwebtoken": "^8.5.9",
    "@types/multer": "^1.4.7",
    "@types/node": "^18.0.0",
    "@types/node-cron": "^3.0.1",
    "@types/papaparse": "^5.3.7",
    "@types/passport": "^1.0.12",
    "@types/passport-google-oauth20": "^2.0.11",
    "@types/puppeteer": "^5.4.4",
    "@types/redux-state-sync": "^3.1.5",
    "@types/tunnel-ssh": "^4.1.1",
    "concurrently": "^6.2.1",
    "nodemon": "^2.0.19",
    "rimraf": "^4.4.1",
    "ts-node": "^10.9.1",
    "tsc-alias": "^1.8.5",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^4.5.4",
    "mongodb": "^5.2.0"
  },
  "type": "module"
}
