{
  "name": "server",
  "version": "1.0.0",
  "description": "server built with mongodb nodejs and changestream",
  "main": "server.js",
  "scripts": {
    "client-install": "cd ../mern-project && npm install",
    "server-install": "npm install",
    "server": "nodemon server.js",
    "client": "cd ../mern-project && npm start",
    "start": "node server.js",
    "dev": "concurrently \"npm run server\" \"npm run client\" \"npm run watch\"",
    "prod": "concurrently \"npm run start\" \"npm run client\"",
    "deploy": "cd ../deploy && node cmd.js",
    "watch": "tsc --watch",
    "clean": "rm -rf node_modules package-lock.json",
    "rmPdf": "cd ./bin/pdfGenerator/pdf && rm ./*"
  },
  "imports": {
    "#root/*": "./*",
    "#rootTS/*": "./dist/*",
    "#bin/*": "./bin/*",
    "#models/*": "./lib/models/*",
    "#routes/*": "./lib/routes/*",
    "#query/*": "./lib/query/*",
    "#wmsTS/*": "./dist/wms/*",
    "#middleware/*": "./lib/middleware/*",
    "#amz/*": "./bin/amazonSP/*",
    "#dist/*": "./dist/*",
    "#routesV1/*": "./dist/lib/routes/*"
  },
  "author": "William Q",
  "license": "MIT",
  "dependencies": {
    "amazon-sp-api": "^0.8.0",
    "aws-sdk": "^2.1277.0",
    "axios": "^0.26.1",
    "bcryptjs": "^2.4.3",
    "concurrently": "^6.2.1",
    "dotenv": "^16.0.0",
    "exceljs": "^4.3.0",
    "express": "^4.17.1",
    "fs-extra": "^10.1.0",
    "googleapis": "^95.0.0",
    "handlebars": "^4.7.7",
    "JSONStream": "^1.3.5",
    "jsonwebtoken": "^9.0.0",
    "moment": "^2.29.4",
    "mongodb": "^4.10.0",
    "mongoose": "^6.9.0",
    "multer": "^1.4.5-lts.1",
    "node-cron": "^3.0.0",
    "node-fetch": "^3.2.10",
    "node-rsa": "^1.1.1",
    "nodemailer": "^6.7.8",
    "puppeteer": "^13.1.3",
    "socket.io": "^4.5.1",
    "tunnel-ssh": "^4.1.6"
  },
  "devDependencies": {
    "@types/express": "^4.17.14",
    "@types/fs-extra": "^9.0.13",
    "@types/jsonwebtoken": "^8.5.9",
    "@types/module-alias": "^2.0.1",
    "@types/multer": "^1.4.7",
    "@types/node": "^18.0.0",
    "@types/node-cron": "^3.0.1",
    "@types/papaparse": "^5.3.7",
    "@types/puppeteer": "^5.4.4",
    "@types/tunnel-ssh": "^4.1.1",
    "nodemon": "^2.0.19",
    "ts-node": "^10.5.0",
    "tsconfig-paths": "^4.1.0",
    "typescript": "^4.5.4"
  },
  "type": "module"
}
