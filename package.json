{
  "name": "server",
  "version": "1.0.0",
  "description": "server built with mongodb nodejs and changestream",
  "main": "server.js",
  "scripts": {
    "client-install": "cd ./mern-project && npm install",
    "server-install": "npm install",
    "server": "cd ./server && nodemon server.js",
    "client": "cd ./mern-project && npm start",
    "start": "cd ./server && node server.js",
    "debug": "cd ./server && node ./server/backend_control/server.js",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "prod": "concurrently \"npm run start\" \"npm run client\"",
    "deploy": "cd ./deploy && node cmd.js",
    "tsc": "tsc",
    "watch": "tsc --watch",
    "clean": "rm -rf node_modules package-lock.json"
  },
  "imports": {
    "#root/*": "./server/*",
    "#bin/*": "./server/bin/*",
    "#models/*": "./server/lib/models/*",
    "#routes/*": "./server/lib/routes/*",
    "#query/*": "./server/lib/query/*",
    "#wms/*": "./server/wms/*",
    "#middleware/*": "./server/lib/middleware/*",
    "#amz/*": "./server/bin/amazonSP/*",
    "#dist/*": "./server/dist/*",
    "#routesV2/*": "./server/dist/lib/routes/*",
    "#rootTS/*":"./server/dist/*"
  },
  "author": "William Q",
  "license": "MIT",
  "dependencies": {
    "JSONStream": "^1.3.5",
    "amazon-sp-api": "^0.7.6",
    "axios": "^0.26.1",
    "bcryptjs": "^2.4.3",
    "concurrently": "^6.2.1",
    "dotenv": "^16.0.0",
    "exceljs": "^4.3.0",
    "express": "^4.17.1",
    "fs-extra": "^10.1.0",
    "googleapis": "^95.0.0",
    "handlebars": "^4.7.7",
    "jsonwebtoken": "^8.5.1",
    "moment": "^2.29.4",
    "mongoose": "^5.13.9",
    "node-cron": "^3.0.0",
    "node-fetch": "^3.2.10",
    "node-rsa": "^1.1.1",
    "nodemailer": "^6.7.8",
    "puppeteer": "^13.1.3",
    "socket.io": "^4.5.1",
    "tunnel-ssh": "^4.1.6"
  },
  "devDependencies": {
    "@types/express": "^4.17.14",
    "@types/fs-extra": "^9.0.13",
    "@types/jsonwebtoken": "^8.5.9",
    "@types/module-alias": "^2.0.1",
    "@types/node": "^18.0.0",
    "@types/node-cron": "^3.0.1",
    "@types/puppeteer": "^5.4.4",
    "@types/tunnel-ssh": "^4.1.1",
    "nodemon": "^2.0.19",
    "ts-node": "^10.5.0",
    "tsconfig-paths": "^4.1.0",
    "typescript": "^4.5.4"
  },
  "type": "module"
}